    "lint-staged": "8.1.5",
    "prettier": "^1.16.4",
    "protractor": "^5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "typescript": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  }
